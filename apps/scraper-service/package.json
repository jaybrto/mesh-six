{
  "name": "@mesh-six/scraper-service",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "dapr-dev": "dapr run --app-id scraper-service --app-port 3000 --resources-path ./components -- bun run dev",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@mesh-six/core": "workspace:*",
    "hono": "^4.7.4",
    "playwright": "^1.50.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.4",
    "typescript": "^5.7.3"
  }
}
